<project name="antLbiBuilder" default="default">
	<description>
            jar builder for antLib
    </description>

	<!-- ================================= 
          target: default              
         ================================= -->
	<target name="default" depends="depends" description="jar builder for antLib">
		<jar jarfile="${basedir}/lib/sideAntTasks.jar" basedir="${basedir}/bin-ant">			
		</jar>
	</target>

	<!-- - - - - - - - - - - - - - - - - - 
          target: depends                      
         - - - - - - - - - - - - - - - - - -->
	<target name="depends">
	</target>

</project>
