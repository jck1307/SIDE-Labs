<?eclipse version="3.2"?>
<plugin>
  <extension point="com.bluexml.side.Application.com_bluexml_application_configuration">
    <metamodel description="Check this to generate class models on leading frameworks.&lt;br&gt; This Class Metamodel allows to model Application Object structure and persistence.&lt;br&gt;SIDE provides the modeler to graphically produce class models and various generators to instantiate these models on leading frameworks." id="http://www.kerblue.org/class/1.0" name="KSR500 Data" url="http://www.kerblue.org/class/1.0">
      <technology description="Generate documentation (in opendocument) for the models of the metamodel." id="opendocument" name="OpenDocument" url="http://www.odfalliance.org/">
        <technologyVersion id="opendocument1_2" version="OpenDocument 1.2">
          <generatorVersion class="com.bluexml.side.clazz.generator.documentation.ClassDocumentationGenerator" hidden="hidden and used by default" id="com.bluexml.side.Class.generator.documentation" version="SIDE Class Documentation Generator">
            <option defaultOption="true" documentation="if enabled generator add model screenShoot" key="doc.gen.screenshoot" label="includes screen shoots" />
          </generatorVersion>
        </technologyVersion>
      </technology>
    </metamodel>
  </extension>
</plugin>

